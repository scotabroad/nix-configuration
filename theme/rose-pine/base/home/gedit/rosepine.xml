<?xml version="1.0" encoding="UTF-8"?>
<!--
License could go here...
-->
<style-scheme id="rose-pine-gedit" name="Rosé Pine" version="1.0">
  <description>All natural pine, faux fur and a bit of soho vibes for the classy minimalist</description>

  <!-- Palette -->
  <color name="Base"              value="#191724"/>
  <color name="Surface"           value="#1f1d2e"/>
  <color name="Overlay"           value="#26233a"/>
  <color name="Muted"             value="#6e6a86"/>
  <color name="Subtle"            value="#908caa"/>
  <color name="Text"              value="#e0def4"/>
  <color name="Love"              value="#eb6f92"/>
  <color name="Gold"              value="#f6c177"/>
  <color name="Rose"              value="#ebbcba"/>
  <color name="Pine"              value="#31748f"/>
  <color name="Foam"              value="#9ccfd8"/>
  <color name="Iris"              value="#c4a7e7"/>
  <color name="Highlight Low"     value="#21202e"/>
  <color name="Highlight Med"     value="#403d52"/>
  <color name="Highlight High"    value="#524f67"/>

  <!-- Settings -->
  <style name="background-pattern" background="Surface"/>
  <style name="bracket-match" foreground="Text" background="Highlight Med"/>
  <style name="bracket-mismatch" foreground="Text" background="Love"/>
  <style name="current-line" foreground="Text" background="Overlay"/>
  <style name="cursor" foreground="Highlight High"/>
  <style name="draw-spaces" foreground="Overlay"/>
  <style name="line-numbers" foreground="Muted" background="Base"/>
  <style name="search-match" foreground="Text" background="Rose"/>
  <style name="secondary-cursor" use-style="cursor"/>
  <style name="selection" foreground="Text" background="Highlight Med"/>
  <style name="selection-unfocused" foreground="Text" background="Iris"/>
  <style name="right-margin" foreground="Highlight High"/>
  <style name="text" foreground="Text" background="Base"/>

  <!--Syntax Highlighting -->
  <!-- Variables -->
  <style name="def:number" foreground="Gold"/>
  <style name="def:string" foreground="Gold"/>
  <style name="def:base-n-integer" use-style="def:number"/>
  <style name="def:character" use-style="def:string"/>
  <style name="def:complex" use-style="def:number"/>
  <style name="def:decimal" use-style="def:number"/>
  <style name="def:floating-point" use-style="def:number"/>
  <style name="def:special-char" foreground="Pine"/>
  <style name="def:variable" foreground="Text"/>
  
  <!-- Constants -->
  <style name="def:boolean" foreground="Rose"/>
  <style name="def:constant" foreground="Text"/>
  <style name="def:namespace" foreground="Iris"/>
  <style name="def:special-constant" foreground="Pine"/>
  
  <!-- Comments -->
  <style name="def:comment" foreground="Muted"/>
  <style name="def:doc-comment" foreground="Muted"/>
  <style name="def:doc-comment-element" foreground="Muted"/>
  <style name="def:net-address-in-comment" foreground="Muted" underline="true"/>
  <style name="def:note" foreground="Muted"/>
  
  <!-- Keywords -->
  <style name="def:class" foreground="Rose"/>
  <style name="def:keyword" foreground="Pine"/>
  <style name="def:attribute" foreground="Rose"/>
  <style name="def:builtin" foreground="Love"/>
  <style name="def:identifier" foreground="Pine"/>
  <style name="def:preprocessor" foreground="Pine"/>
  <style name="def:reserved" foreground="Pine"/>
  <style name="def:shebang" foreground="Iris"/>
  <style name="def:statement" foreground="Pine"/>
  <style name="def:tag" foreground="Pine"/>
  <style name="def:type" foreground="Foam"/>
  
  <!-- Logicals -->
  <style name="def:error" foreground="Love"/>
  <style name="def:function" foreground="Pine"/>
  <style name="def:operator" foreground="Subtle"/>

  <!-- Formats -->
  <style name="def:underlined" underline="true"/>
  <style name="def:warning" foreground="Gold"/>
</style-scheme>
